    <style>
        .is_loading header {
            display: none;

        }

        .is_loading #body {
            display: none;
        }

        /*= Preload */
        @keyframes widthSegment {
            from {
                width: 0%;
            }

            to {
                width: 100%;
            }
        }

        @keyframes heightSegment {
            from {
                height: 0%;
            }

            to {
                height: 100%;
            }
        }

        @keyframes preloading {
            from {
                width: 100px;
                height: 100px;
                top: 50%;
                left: 50%;
                margin: -50px 0 0 -50px;
            }

            to {
                width: 100%;
                height: 100%;
                top: 0;
                left: 0;
                margin: 0;
            }
        }

        #preload {
            width: 100%;
            height: 100%;
            position: absolute;
            left: 0;
            top: 0;
            z-index: 99999999999999999;
        }

        #preload.close {
            display: none;
        }

        #preloader {
            width: 7rem;
            height: 7rem;
            position: absolute;
            top: 50%;
            left: 50%;
            margin-top: -3.5rem;
            margin-left: -3.5rem;
            animation: preloading 0.25s ease-out 1.23s both;
        }

        #preloader-content {
            width: 100%;
            height: 100%;
            position: absolute;
            box-shadow: white 0px 0px 0px 2005px;

        }

        .dark #preloader-content {
            box-shadow: black 0px 0px 0px 2005px;
        }

        #preloader:before,
        #preloader:after,
        #preloader-content:before,
        #preloader-content:after {
            content: " ";
            display: block;
            background-color: black;
            position: absolute;
        }

        .dark #preloader:before,
        .dark #preloader:after,
        .dark #preloader-content:before,
        .dark #preloader-content:after {
            background-color: white;

        }

        #preloader:before {
            width: 0;
            height: 1px;
            animation: widthSegment 0.25s linear both;
        }

        #preloader:after {
            height: 0;
            right: 0;
            width: 1px;
            animation: heightSegment 0.25s linear 0.25s both;
        }

        #preloader-content:before {
            width: 0;
            right: 0;
            bottom: 0;
            height: 1px;
            animation: widthSegment 0.25s linear 0.5s both;
        }

        #preloader-content:after {
            height: 0;
            left: 0;
            bottom: 0;
            width: 1px;
            animation: heightSegment 0.25s linear 0.75s both;
        }
    </style>
    </head>
